select * from "ADMINISTRADOR";
select * from "CLIENTE";
select * from "ADUANA";
select * from "ALMACEN";

--USANDO JOINS-- 
-- VER LOS CLIENTES DE UN ADMINISTRADOR
SELECT "ADMINISTRADOR"._NOMBRE, "CLIENTE"._NOMBRE
FROM "ADMINISTRADOR"
INNER JOIN "CLIENTE"
ON "ADMINISTRADOR"._ID = "CLIENTE"."_fkAdminCliente"
WHERE "ADMINISTRADOR"._ID=2;

-- VER LOS ALMACENES DE UNA ADUANA
SELECT "ADUANA"._NOMBRE as "Aduana", "ALMACEN"."_nombreAlmacen" as "Almacen"
FROM "ADUANA"
INNER JOIN "ALMACEN"
ON "ADUANA"._ID = "ALMACEN"."_fkAduanaAlmacen"
WHERE "ADUANA"._ID=2;

